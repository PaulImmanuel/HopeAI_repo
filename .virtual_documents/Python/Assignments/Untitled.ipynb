class OddEven():
    def OddEven():
        numb = int(input("Enter a number:"))
        if((numb % 2) == 0):
            print(numb,"is Even number")
        else:
            print(numb,"is Odd number")  


OddEven.OddEven()


class EligiblityForMarriage:
    def Eligible():
        gender = input("Enter your Gender:")
        age = int(input("Enter your Age:"))
        if(gender == 'Male'):
            if(age >=21 ):
                print("ELIGIBLE")
            else:
                print("NOT ELIGIBLE")
        elif(gender == 'Female'):
            if(age >=18 ):
                print("ELIGIBLE")
            else:
                print("NOT ELIGIBLE")


EligiblityForMarriage.Eligible()


